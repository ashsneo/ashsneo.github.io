{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@assets/*": ["assets/*"],
      "@config": ["config.ts"],
      "@types": ["types.ts"],
      "@components/*": ["components/*"],
      "@content/*": ["content/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@plugins/*": ["plugins/*"]
    },
    "types": ["astro/client"],
    "strictNullChecks": true
  }
}